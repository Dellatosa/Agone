<div class="carte instrument-desaccord" data-owner-id="{{owner}}">
    <div class="entete">
        <!-- <img src="{{img}}" title="{{name}}" height="32" width="32"/> -->
        <span>{{localize "agone.actors.jetDesaccord"}}</span>
    </div>
    <div class="grid-2-chk">
        <label>{{ localize "agone.chat.utiliseHeroisme"}}</label>
        <input class="util-hero" type="checkbox" name="utiliseHeroisme"/>
    </div>
    <div>Instrument utilisé pour le jet de désaccord</div>
     <div class="boutons-carte">
        {{#each instruments as | instrument | }}
            <button class="jet-desaccord" data-instrument="{{instrument.label}}">{{configLocalize "instruments" instrument.label}}</button>
        {{/each}}
    </div>
</div>